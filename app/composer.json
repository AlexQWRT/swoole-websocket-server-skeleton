{
    "name": "alexqwrt/swoole-websocket-server-skeleton",
    "description": "The skeleton for websocket server in swoole",
    "keywords": ["swoole", "php"],
    "license": "MIT",
    "type": "template",
    "require": {
        "php": "^8.2",
        "ext-swoole": "*",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
